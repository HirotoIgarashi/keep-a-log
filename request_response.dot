digraph http {
  // graphの定義
  graph [
    rankdir = LR;
  ];
  // nodeの定義
  node [
    fontname="IPAPGothic"
  ];

  // edgeの定義
  edge [
    fontname="IPAPGothic"
  ];

  "Webブラウザ" [shape = box];
  "Webサーバ" [shape = box];

  // edgeの定義
  "Webブラウザ" -> "Webサーバ" [label = "リクエストメッセージ"];
  "Webサーバ" -> "Webブラウザ" [label = "レスポンスメッセージ"];
}
